{
  "name": "PromptPro",
  "author": "ziinc",
  "description": "A ChatGPT prompt manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:app && (cd docs; npm run build)",
    "build:app": "npm run build:app:chrome",
    "build:app:firefox": "tsc && TARGET_BROWSER=firefox vite build",
    "build:app:chrome": "tsc && TARGET_BROWSER=chrome vite build",
    "preview": "vite preview",
    "test": "vitest",
    "serve:firefox": "web-ext run --start-url \"https://example.com\"  --source-dir ./dist/",
    "serve:chrome": "web-ext run -t chromium --start-url \"https://example.com\" --source-dir ./dist/"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@monaco-editor/react": "^4.5.1",
    "@mui/material": "^5.14.4",
    "@supabase/supabase-js": "^2.22.0",
    "antd": "^5.3.2",
    "axios": "^1.3.4",
    "batching-toposort": "^1.2.0",
    "common": "file:./common",
    "dayjs": "^1.11.7",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lucide": "^0.125.0",
    "lucide-react": "^0.125.0",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "reactflow": "^11.7.0",
    "swr": "^2.2.2",
    "webextension-polyfill": "^0.10.0",
    "wouter": "^2.10.0"
  },
  "devDependencies": {
    "@samrum/vite-plugin-web-extension": "^5.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/chrome": "^0.0.228",
    "@types/lodash": "^4.14.194",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/webextension-polyfill": "^0.10.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "^10.4.14",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.21",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.4.9",
    "vitest": "^0.32.2",
    "web-ext": "^7.6.2"
  }
}
